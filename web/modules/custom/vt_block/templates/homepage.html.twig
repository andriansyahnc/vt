{#
/**
* @file
* Default theme implementation to display a block.
*
* @see template_preprocess_block()
*
* @ingroup themeable
*/
#}
{% set classes = [
    'clear-both',
] %}
<div{{ attributes.addClass(classes) }}>
  <div style="text-align: center; width: 100%; background-image: url('{{ content.image }}')">
    <div>
      <h2>{{ content.title }}</h2>
    </div>
    <div>
      <h4>{{ content.subtitle }}</h4>
    </div>
  </div>
</div>
